-- üìå Script ‡∏£‡∏ß‡∏°‡∏£‡∏∞‡∏ö‡∏ö: ‡πÄ‡∏î‡∏¥‡∏ô‡∏™‡∏∏‡πà‡∏° + ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î + ‡∏´‡∏°‡∏∏‡∏ô‡∏Å‡∏•‡πâ‡∏≠‡∏á + ‡∏£‡∏µ‡∏ï‡∏±‡∏ß‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏ô + Hop Low ‡∏´‡∏≤‡∏Å‡∏°‡∏µ‡∏à‡∏±‡∏Å‡∏£‡∏¢‡∏≤‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡πÉ‡∏Å‡∏•‡πâ

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")
local lp = Players.LocalPlayer

-- üîÅ ‡∏£‡∏µ‡∏ï‡∏±‡∏ß‡∏ñ‡πâ‡∏≤ AFK 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
local lastPosition = nil
local lastMoved = tick()

RunService.Heartbeat:Connect(function()
    if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
        local pos = lp.Character.HumanoidRootPart.Position
        if lastPosition and (pos - lastPosition).Magnitude > 0.1 then
            lastMoved = tick()
        end
        lastPosition = pos

        if tick() - lastMoved > 30 then
            local humanoid = lp.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.Health = 0 -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡πà‡∏≤‡∏á
            end
            lastMoved = tick()
        end
    end
end)

-- üö¥‚Äç‚ôÇÔ∏è ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏à‡∏±‡∏Å‡∏£‡∏¢‡∏≤‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡πÉ‡∏Å‡∏•‡πâ‡πÅ‡∏•‡πâ‡∏ß Hop
local function isBikeNearby()
    if not lp.Character or not lp.Character:FindFirstChild("HumanoidRootPart") then return false end
    local myPos = lp.Character.HumanoidRootPart.Position
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("VehicleSeat") and (v.Position - myPos).Magnitude < 25 then
            return true
        end
    end
    return false
end

RunService.Heartbeat:Connect(function()
    if isBikeNearby() then
        local servers = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
        for _, s in pairs(servers.data) do
            if s.playing < s.maxPlayers then
                TeleportService:TeleportToPlaceInstance(game.PlaceId, s.id, lp)
                break
            end
        end
    end
end)

-- üö∂‚Äç‚ôÇÔ∏è ‡πÄ‡∏î‡∏¥‡∏ô‡∏™‡∏∏‡πà‡∏° + ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î + ‡∏´‡∏°‡∏∏‡∏ô‡∏Å‡∏•‡πâ‡∏≠‡∏á + Hop Low ‡∏ó‡∏∏‡∏Å 2.5 ‡∏ä‡∏±‡πà‡∏ß‡πÇ‡∏°‡∏á
local function randomMove()
    local humanoid = lp.Character and lp.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local action = math.random(1, 4)
        if action == 1 then
            humanoid:Move(Vector3.new(1, 0, 0), false) -- ‡πÄ‡∏î‡∏¥‡∏ô‡∏Ç‡∏ß‡∏≤
        elseif action == 2 then
            humanoid:Move(Vector3.new(-1, 0, 0), false) -- ‡πÄ‡∏î‡∏¥‡∏ô‡∏ã‡πâ‡∏≤‡∏¢
        elseif action == 3 then
            humanoid:Move(Vector3.new(0, 0, 1), false) -- ‡πÄ‡∏î‡∏¥‡∏ô‡∏´‡∏ô‡πâ‡∏≤
        elseif action == 4 then
            humanoid:Move(Vector3.new(0, 0, -1), false) -- ‡∏ñ‡∏≠‡∏¢‡∏´‡∏•‡∏±‡∏á
        end
    end
end

-- ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏ó‡∏∏‡∏Å‡πÜ 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
while true do
    task.wait(10)
    if lp.Character and lp.Character:FindFirstChildOfClass("Humanoid") then
        lp.Character.Humanoid.Jump = true
    end
end

-- ‡∏´‡∏°‡∏∏‡∏ô‡∏Å‡∏•‡πâ‡∏≠‡∏á‡∏ó‡∏∏‡∏Å 8 ‡∏ß‡∏¥
task.spawn(function()
    while true do
        task.wait(8)
        if workspace.CurrentCamera then
            workspace.CurrentCamera.CFrame *= CFrame.Angles(0, math.rad(math.random(-40, 40)), 0)
        end
    end
end)

-- Hop ‡πÑ‡∏õ‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏Ñ‡∏ô‡∏ô‡πâ‡∏≠‡∏¢‡∏ó‡∏∏‡∏Å 2.5 ‡∏ä‡∏±‡πà‡∏ß‡πÇ‡∏°‡∏á
task.spawn(function()
    while true do
        task.wait(9000) -- 2.5 ‡∏ä‡∏±‡πà‡∏ß‡πÇ‡∏°‡∏á = 9000 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
        local servers = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
        for _, s in pairs(servers.data) do
            if s.playing < s.maxPlayers then
                TeleportService:TeleportToPlaceInstance(game.PlaceId, s.id, lp)
                break
            end
        end
    end
end)
